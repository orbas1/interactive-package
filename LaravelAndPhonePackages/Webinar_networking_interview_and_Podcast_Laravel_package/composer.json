{
    "name": "jobi/webinar-networking-interview-podcast",
    "description": "Comprehensive Laravel package providing webinars, networking, podcasts, and interview workflows for LinkedIn-style platforms.",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "illuminate/support": "^10.0",
        "illuminate/auth": "^10.0",
        "illuminate/routing": "^10.0",
        "illuminate/database": "^10.0",
        "illuminate/validation": "^10.0",
        "illuminate/queue": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Jobi\\WebinarNetworkingInterviewPodcast\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jobi\\WebinarNetworkingInterviewPodcast\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jobi\\WebinarNetworkingInterviewPodcast\\Providers\\WebinarPodcastNetworkingInterviewServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
